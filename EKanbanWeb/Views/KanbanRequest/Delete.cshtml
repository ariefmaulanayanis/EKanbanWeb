@model EKanbanWeb.Models.KanbanRequest
@{
    Layout = null;
}

<div>Are you sure you want to delete this data?</div>
<div>
    <hr />
    <dl class="row">
        @Html.HiddenFor(a => a.KanbanReqId)
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.TagRequestNo)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.TagRequestNo)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.RequestDateTime)
        </dt>
        <dd class="col-sm-7">
            @Model.RequestDateTime.ToString("dd-MMM-yyyy hh:mm:ss")
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.PartFGId)
        </dt>
        <dd class="col-sm-7">
            @ViewBag.PartFGNo (@ViewBag.PartFGName)
        </dd>
        <dt class="col-sm-5">
            Line No
        </dt>
        <dd class="col-sm-7">
            @ViewBag.LineNo (@ViewBag.LineName)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.StatusId)
        </dt>
        <dd class="col-sm-7">
            @ViewBag.StatusName
        </dd>
    </dl>
    <form asp-action="Delete" asp-controller="KanbanRequest" method="post" onsubmit="return jQueryAjaxPost(this,'Delete');">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <input type="hidden" id="id" value="@Model.KanbanReqId" />
    </form>
</div>